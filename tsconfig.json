{
  "compilerOptions": {
    "target": "ES2015",
    "module": "AMD",
    "moduleResolution": "node",
    "outDir": "./www/js",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "lib": ["DOM", "ES5", "ES6", "ES2017.Object"]
  },
  "include": [
    "src"
  ],
  "types": [
    "greensock",
    "jquery",
    "vue"
  ]
}
